{
	"id": "manual-trigger",
	"version": "1.0.0",
	"name": "Manual Trigger",
	"category": "Core",
	"description": "Starts the flow immediately when the graph runs.",
	"ui": {
		"color": "#10b981",
		"icon": "play"
	},
	"execution": {
		"timeoutMs": 1000
	},
	"ports": {
		"inputs": [],
		"outputs": [
			{
				"name": "output",
				"type": "any",
				"description": "Trigger data"
			}
		]
	},
	"settings": [
		{
			"name": "payload",
			"type": "json",
			"label": "Payload",
			"defaultValue": { "message": "Hello World" }
		}
	]
}
